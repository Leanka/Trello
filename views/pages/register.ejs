<% include ../partials/landing-header %>
       <div class="row">
           <div class="content container">
               <div class="well">
                    <h1 style="text-align: center">Register</h1>
                    <div style="width: 30%; margin: 25px auto;">
                    <form action="/register" method="POST">
                    <div class="form-group">
                        <input class="form-control" type="text" name="username" placeholder="username" required autofocus autocomplete="on" maxlength="20">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="password" name="password" placeholder="password" required maxlength="20" minlength="6">
                    </div>
                    <div class="form-group">
                        <button class="btn btn-lg btn-primary btn-block">Continue</button>
                    </div>
                    </form>
                    <a href="/">Go Back</a>
                    </div> 
               </div>
           </div>
        </div>
        <% include ../partials/footer %>
